{
  "Logging": {
    "LogLevel": {
      //LogLevel is used to control how much log data is logged or displayed
      "Default": "Trace",
      //All categories on log in need to display.
      "Microsoft": "Warning"
    }
  },

  "Data1": {
    "ConnectionStrings": {
      //Multiple Active Result Sets (MARS) is a feature that works with SQL Server to allow the execution of multiple batches
      //on a single connection. When MARS is enabled for use with SQL Server, each command object used adds a session to the connection.
      //Summing up:Trusted_Connection=Yes" specifies that a user account is used to establish this connection.
      //If you specify either Trusted_Connection=True; or Integrated Security=SSPI; or Integrated Security=true; in your connection string
      //THEN (and only then) you have Windows Authentication happening. Any user id= setting in the connection string will be ignored.
      //If you DO NOT specify either of those settings
      // then you DO NOT have Windows Authentication happening (SQL Authentication mode will be used)
      "MyConnStr": "Data Source=DS12\\SQLEXPRESS;Database=MyDatabase;Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  },

  "Data2": {
    "ConnectionStrings": {
      "SongsStoreIdentity": "Data Source=DS12\\SQLEXPRESS;Database=Identity;Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  }


